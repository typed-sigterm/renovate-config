{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    ":dependencyDashboardApproval",
    ":semanticCommitTypeAll(chore)",
    "schedule:weekly",
    "group:allNonMajor",
    "workarounds:typesNodeVersioning"
  ],
  "labels": [
    "dependencies"
  ],
  "packageRules": [
    {
      "matchPackageNames": ["nuxt"],
      "allowedVersions": "<= 3.13.1 || > 3.13.2"
    }
  ]
}
