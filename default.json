{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    "docker:pinDigests",
    "helpers:pinGitHubActionDigests",
    "schedule:weekly",
    "group:allNonMajor",
    ":configMigration",
    ":dependencyDashboardApproval",
    ":semanticCommitTypeAll(chore)"
  ],
  "ignorePresets": [
    "group:recommended"
  ],
  "labels": [
    "dependencies"
  ],
  "packageRules": [
    {
      "matchPackageNames": ["nuxt"],
      "allowedVersions": "<= 3.13.1 || > 3.13.2"
    }
  ]
}
